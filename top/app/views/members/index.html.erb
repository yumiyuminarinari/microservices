<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member['name'] %></td>
        <td><%= member['age'] %></td>
        <td><%= link_to 'Show', member_path(member['id']) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
